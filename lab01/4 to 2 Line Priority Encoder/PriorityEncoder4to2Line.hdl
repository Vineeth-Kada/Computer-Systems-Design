CHIP PriorityEncoder4to2Line {
    IN x0,x1,x2,x3;
    OUT y0,y1;

    PARTS:
    Not(in=x0, out=nx0);
    Not(in=x1, out=nx1);
    Not(in=x2, out=nx2);
    Not(in=x3, out=nx3);
    
    And(a=nx0,b=nx1,out=y1);

    And(a=nx0,b=x1,out=temp1);
    And(a=nx0,b=nx2,out=temp2);
    Or(a=temp1,b=temp2,out=y0);
}